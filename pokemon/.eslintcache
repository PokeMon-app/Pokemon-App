React, {useState} from 'react';\nimport pokeball from './Images/pokeball1.png';\nimport allTypes from './Images/all_types.png';\nimport {FontAwesomeIcon} from '@fortawesome/react-fontawesome'\nimport {faSearch} from '@fortawesome/free-solid-svg-icons'\nimport {faCaretRight} from '@fortawesome/free-solid-svg-icons'\nimport {faUserCircle} from '@fortawesome/free-solid-svg-icons'\nimport {Link} from 'react-router-dom';\nimport Modal from \"react-bootstrap/Modal\";\nimport TypeModal from \"./TypeModal\";\n\nconst NavbarIndex = () => {\n    const [showModal, setShowModal] = useState(false);\n    const [isOpen, setIsOpen] = useState(false);\n    const [searchStatus, setSearchStatus] = useState(false);\n\n    const callback = (isOpen, showModal) => {\n        setIsOpen(!isOpen);\n        setShowModal(!showModal);\n    }\n\n    const searchTransition = (searchStatus) => {\n        setSearchStatus(!searchStatus)\n        console.log(searchStatus)\n        if(isOpen == true) {\n            setSearchStatus(!searchStatus);\n\n        }\n        else if(isOpen == false) {\n            setSearchStatus(!searchStatus)\n        }\n    }\n\n    const RenderDropdown = () => {\n        if (isOpen === true) {\n            return (\n                <div className={'pokeball-menu'}>\n                        <div className={\"dropdown-link align-self-center\"}>\n                            <Link to={\"/profile\"} onClick={() => setIsOpen(!isOpen)}\n                            ><FontAwesomeIcon icon={faUserCircle} size=\"2x\" className={\"profile-link\"}></FontAwesomeIcon></Link>\n                        </div>\n                        <div className={\"dropdown-link d-flex\" + (searchStatus ? \"\" : \" center-icon\")}>\n                            <FontAwesomeIcon icon={faSearch} size=\"2x\" onClick={() => {searchTransition(searchStatus);}}\n                            className={\"nav-links circular align-self-center hover\" + (searchStatus ? \" search-hide\" : \" search-show\")}></FontAwesomeIcon>\n                            <div className={\"search-container d-flex align-self-center\" + (!searchStatus ? \" search-hide\" : \" search-show\")}>\n                                <FontAwesomeIcon icon={faCaretRight} size=\"2x\" className={\"align-self-center mr-1 hover \" + (!searchStatus ? \"search-hide\" : \"search-show\")} onClick={() => {setSearchStatus(false)}}></FontAwesomeIcon>\n                                <input className={\"nav-links round-input\" + (!searchStatus ? \" search-hide\" : \" search-show\")} placeholder={\"Search...\"} type={\"text\"}/>\n                            </div>\n                        </div>\n                        <div className={\"dropdown-link align-self-center\"}>\n                                <img className={(!showModal ? \"all-types\" : \"all-types-active\") + \" p-1\"} onClick={() => setShowModal(!showModal)\n                                } src={allTypes} alt=\"Filter by types\"/>\n                        </div>\n                </div>\n            )\n        }\n        return null;\n    }\n\n    const pokeballImg = () => {\n        return (\n            <button className={\"buttonInner\" + (!isOpen ? \" closedPokeball\" : \" openPokeball\")}\n                    type={'button'} onClick={() => {\n                setIsOpen(!isOpen)\n                if(isOpen != true) {\n                    setSearchStatus(false)\n                }\n            }}>\n                <img className={\"pokeball\"} src={pokeball} alt=\"pokeball\"/>\n            </button>\n        )\n    }\n\n    return [\n\n        (\n        <React.Fragment>\n            <nav className={\"nav d-flex\"}>\n                <Link to={\"/\"}>\n                    <h4>PokeTeam Builder</h4>\n                </Link>\n                <Link to={\"/register\"}>\n                    <p>Register</p>\n                </Link>\n                <Link to={\"/sign-in\"}>\n                    <p>Sign In</p>\n                </Link>\n                <div className={\"ml-auto mr-3 my-auto drop-icon\"}>\n                    <div className={\"\"}>\n                        {pokeballImg()}\n                    </div>\n                    <RenderDropdown/>\n                </div>\n            </nav>\n\n            <Modal\n                show={showModal}\n                onHide={() => setShowModal(!showModal)}\n                dialogClassName=\"modal-90w\"\n                aria-labelledby=\"example-custom-modal-styling-title\"\n            >\n                <Modal.Header closeButton>\n                    <Modal.Title id=\"example-custom-modal-styling-title\">\n                        Custom Modal Styling\n                    </Modal.Title>\n                </Modal.Header>\n                <Modal.Body>\n                    <TypeModal parentCallback={callback}/>\n                </Modal.Body>\n            </Modal>\n        </React.Fragment>\n    )]\n}\n\nexport default NavbarIndex;","/Users/caleb/IdeaProjects/Pokemon-App/pokemon/src/components/Register.js",[],"/Users/caleb/IdeaProjects/Pokemon-App/pokemon/src/components/Welcome.js",[],"/Users/caleb/IdeaProjects/Pokemon-App/pokemon/src/history.js",[],"/Users/caleb/IdeaProjects/Pokemon-App/pokemon/src/components/SignIn.js",[],"/Users/caleb/IdeaProjects/Pokemon-App/pokemon/src/components/UserProfile.js",[],"/Users/caleb/IdeaProjects/Pokemon-App/pokemon/src/components/TypeModal.js",[],"/Users/caleb/IdeaProjects/Pokemon-App/pokemon/src/components/PokeballMenu.js",["63","64"],"/Users/caleb/IdeaProjects/Pokemon-App/pokemon/src/components/NavbarStatePlay.js",["65","66","67"],"import React, {useState} from 'react';\nimport {Link} from \"react-router-dom\";\nimport Modal from \"react-bootstrap/Modal\";\nimport TypeModal from \"./TypeModal\";\nimport pokeball from \"./Images/pokeball1.png\";\n\nclass NavbarStatePlay extends React.Component {\n    //accesses the element that container is set to\n    container = React.createRef();\n    //state change to set default value of the dropdown to false;\n    state = {\n        open: false,\n    };\n    //function that changes the state change when the button is clicked\n    handleButtonCLick = () => {\n        console.log(this.container)\n        this.setState(state => {\n            return {\n                open: !state.open,\n            };\n        });\n    }\n\n    //events listeners that will run handleClickOutside\n    componentDidMount = () => {\n        document.addEventListener(\"mousedown\", this.handleClickOutside);\n    };\n    componentWillUnmount() {\n        document.removeEventListener(\"mousedown\", this.handleClickOutside);\n    }\n    //will change the state when anywhere on the screen is clicked\n    handleClickOutside = event => {\n\n        if (this.container.current && !this.container.current.contains(event.target)) {\n            this.setState({\n                open: false,\n            });\n        }\n    };\n\n\n    pokeballImg() {\n        return (\n            <button className={\"buttonInner openPokeball\"} type={'button'} onClick={this.handleButtonCLick} ref={this.container}>\n                <img className={\"pokeball\"} src={pokeball} alt=\"pokeball\"/>\n            </button>\n        )\n    }\n\n    render() {\n        return (\n        <div>\n            <nav className={\"nav d-flex\"}>\n                <Link to={\"/\"}>\n                    <h4>PokeTeam Builder</h4>\n                </Link>\n                <Link to={\"/register\"}>\n                    <p>Register</p>\n                </Link>\n                <Link to={\"/sign-in\"}>\n                    <p>Sign In</p>\n                </Link>\n\n                <div className={\"ml-auto mr-3 drop-icon\"}\n                     >\n                    <div className={\"\"}>\n                        {this.pokeballImg()}\n                    </div>\n                    {this.state.open && (\n                    <div className={\"pokeball-menu\"}>\n                        <ul>\n                            <li className={\"dropd